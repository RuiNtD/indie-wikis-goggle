{{#each langs as |lang|}}

! {{{lang.name}}}
{{#each this.sites as |site|}}

{{#with site.dest as |dest|}}
! {{{dest.name}}}
{{{dest.path}}}$site={{{dest.site}}}
{{/with}}
{{#each origins as |origin|}}
{{{origin.path}}}$site={{{origin.site}}},discard
{{/each}}
{{/each}}
{{/each}}
